{
  "name": "phdcc-papers",
  "version": "2.0.34",
  "description": "Papers journal/conference abstract and paper submission and review system",
  "author": "chriscant",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "build-azure": "nuxt generate",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "static": "npx nuxi generate",
    "lint": "eslint --ext .js,.jsx,.ts,.vue .",
    "lintfix": "eslint --fix --ext .js,.jsx,.ts,.vue .",
    "clienttest": "env-cmd -f .env.clienttest nuxt dev",
    "cypress": "cypress run",
    "test": "npx cypress open",
    "waitonapi": "wait-on http://localhost:9000/ && echo 'READY'",
    "waitonclient": "wait-on http://localhost:3000/ && echo 'READY'",
    "runner": "cross-env PORT=n node .output/server/index.mjs"
  },
  "engines": {
    "node": "^18.0.0"
  },
  "comments": {
    "0": "engines/node specifies azure static web app runtime used",
    "1": "pinia-plugin-persist package code in utils\\pinia-plugin-persist.ts from v1.0.0 https://github.com/Seb-L/pinia-plugin-persist",
    "3": "https://www.npmjs.com/package/@fortawesome/vue-fontawesome?activeTab=versions",
    "5": "bootstrap-vue-next 0.4.15 Switch to unplugin-icons https://cdmoro.github.io/bootstrap-vue-3/icons/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@pinia/nuxt": "^0.5.1",
    "@popperjs/core": "^2.11.6",
    "@vitejs/plugin-legacy": "^5.2.0",
    "axios": "^1.2.3",
    "jwt-decode": "^4.0.0",
    "path-browserify": "^1.0.1",
    "vue-recaptcha-v3": "^2.0.1"
  },
  "devDependencies": {
    "@bootstrap-vue-next/nuxt": "^0.24.2",
    "@cypress/code-coverage": "^3.10.0",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@types/jest": "^29.2.6",
    "@types/lodash": "^4.14.191",
    "@vue/compiler-sfc": "^3.2.45",
    "bootstrap": "^5.3.0",
    "bootstrap-vue-next": "^0.24.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.1.0",
    "env-cmd": "^10.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nuxt": "^3.11.2",
    "prettier": "^3.0.0",
    "sass": "^1.74.1",
    "unplugin-icons": "^0.19.0",
    "unplugin-vue-components": "^0.27.0",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-istanbul": "^6.0.0",
    "wait-on": "^8.0.0"
  }
}
